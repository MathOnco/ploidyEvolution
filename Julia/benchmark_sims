echo "input, time, simtype, run" > simtimes.txt
for j in 1 2 3
do
for i in v1
do
   	START=$(date +%s)
	julia -p 4 --project=. -L ploidyMovement.jl driver.jl -i input_$i.toml -c birthLandscapeBrainCancer.txt 
	END=$(date +%s)
	echo "$i, $((END-START)), gpu, $j" >> simtimes.txt

   	START=$(date +%s)
	julia -p 4 --project=. -L ploidyMovement_no-gpu.jl driver.jl -i input_$i.toml -c birthLandscapeBrainCancer.txt 
	END=$(date +%s)
	echo "$i, $((END-START)), no_gpu, $j" >> simtimes.txt

	START=$(date +%s)
	julia --project=. -L ploidyMovement_nothing.jl driver.jl -i input_$i.toml -c birthLandscapeBrainCancer.txt 
	END=$(date +%s)
	echo "$i, $((END-START)), nothing, $j" >> simtimes.txt

done
done
